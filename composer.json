{
    "name": "npowest/garden-helper",
    "description": "Emum, Collection, Functions for NPOWEST Garden",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Npowest\\GardenHelper\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Virus3D",
            "email": "viisto@yandex.ru"
        }
    ],
    "require": {
        "php": ">=8.2"
    },
    "require-dev": {
        "nunomaduro/phpinsights": "^2.11",
        "squizlabs/php_codesniffer": "^3.9",
        "phpstan/phpstan": "^1.10",
        "phpmd/phpmd": "^2.15",
        "phpstan/phpstan-strict-rules": "^1.5"
    },
    "config": {
        "platform": {
            "php": "8.2"
        },
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
